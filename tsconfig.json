    "compilerOptions": {
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true,
        "baseUrl": "./",
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "module": "es6",
